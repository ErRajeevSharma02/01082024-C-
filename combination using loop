#include<iostream>
using namespace std;
int main(){

  int n;
  cout<<"Enter the value of N: ";
  cin>>n;
  cout<<endl;

  int r;
  cout<<"Enter the value of R: ";
  cin>>r;

  int a=1;
  for(int i=1;i<=n;i++)
    {
      a=a*i;
    }

  int b=1;
  for(int j=1;j<=r;j++)
    {
      b=b*j;
    }
  int c=1;
  for(int i=1;i<=n-r;i++){
    c= c*i;
  }

  cout<<"nCr of = "<<a/(b*c);




  return 0;
}
