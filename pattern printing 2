#include <iostream>
using namespace std;
int main() {
   



  int rows,col;
   cout<<"Enter the number of rows: ";
   cin>>rows;
   cout<<"Enter the number of columns : ";
   cin>>col;
   cout<<endl;
   
   
   for(int i=1;i<=rows;i++)
   {
     for(int j=1;j<=col;j++)
     {
        cout<<i<<" ";
     }
     cout<<endl;
   }
   cout<<endl;
   
  for(int i=1;i<=rows;i++)
  {
      
      for(int j=1;j<=col;j++)
      {
          cout<<(char )(j+64 )<<" ";
      }
     cout<<endl;   
  }
 cout<<endl;
  
   for(int k=1;k<=rows;k++)
  {
      
      for(int l=1;l<=col;l++)
      {
          cout<<(char )(k+64 )<<" ";
      }
     cout<<endl;   
  }

 cout<<endl;
  
   for(int m=1;m<=rows;m++)
  {
      
      for(int n=1;n<=col;n++)
      {
          cout<<(char )(n+96 )<<" ";
      }
     cout<<endl;   
  }

 cout<<endl;
  
   for(int x=1;x<=rows;x++)
  {
      
      for(int y=1;y<=col;y++)
      {
          cout<<(char )(x+96 )<<" ";
      }
     cout<<endl;   
  }



    return 0;
}
