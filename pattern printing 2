#include<iostream>
using namespace std;
int main()
{
   for(int i=1;i<=4;i++)
   {
     for(int j=1;j<=4;j++)
     {
        cout<<i<<" ";
     }
   }

  int rows,col;
   cout<<"Enter the number of rows: ";
   cin>>rows;
   cout<<"Enter the number of columns : ";
   cin>>col;
   cout<<endl;
   
  for(int i=1;i<=rows;i++)
  {
      
      for(int j=1;j<=col;j++)
      {
          cout<<(char )(j+64 )<<" ";
      }
     cout<<endl;   
  }
 return 0;
}
